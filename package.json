{
  "name": "@reynoldadade/hacked-components",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --emitDeclarationOnly && mv dist/src dist/types && vite build",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/hacked-components.umd.cjs",
  "module": "./dist/hacked-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/hacked-components.es.js",
      "require": "./dist/hacked-components.umd.cjs"
    }
  },
  "dependencies": {
    "hacked-components": "hacked-components-0.0.0.tgz",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
